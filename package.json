{
  "name": "tea-taster",
  "description": "Tea Tasting Notes",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "ng build --configuration production && cap sync",
    "lint": "ng lint",
    "ng": "ng",
    "prepare": "husky install",
    "start": "ng serve --port=8100",
    "test": "ng test",
    "test:ci": "ng test --no-watch",
    "test:debug": "ng test --browsers=Chrome",
    "watch": "ng build --watch --configuration development",
    "android:watch": "ionic capacitor run android --livereload --source-maps --external"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^16.0.1",
    "@angular/core": "^16.0.1",
    "@angular/forms": "^16.0.1",
    "@angular/platform-browser": "^16.0.1",
    "@angular/platform-browser-dynamic": "^16.0.1",
    "@angular/router": "^16.0.1",
    "@angular/service-worker": "^16.0.1",
    "@capacitor/android": "^5.0.0",
    "@capacitor/app": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/preferences": "^5.0.0",
    "@capacitor/share": "^5.0.0",
    "@capacitor/splash-screen": "^5.0.0",
    "@ionic-enterprise/auth": "^5.0.4",
    "@ionic-enterprise/identity-vault": "5.11.1",
    "@ionic/angular": "~7.3.0",
    "ionicons": "~7.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.1",
    "@angular-eslint/builder": "^16.0.1",
    "@angular-eslint/eslint-plugin": "^16.0.1",
    "@angular-eslint/eslint-plugin-template": "^16.0.1",
    "@angular-eslint/schematics": "^16.0.1",
    "@angular-eslint/template-parser": "^16.0.1",
    "@angular/cli": "^16.0.1",
    "@angular/compiler": "^16.0.1",
    "@angular/compiler-cli": "^16.0.1",
    "@angular/language-service": "^16.0.1",
    "@capacitor/assets": "~2.0.4",
    "@capacitor/cli": "^5.0.0",
    "@ionic/angular-toolkit": "~10.0.0",
    "@ionic/prettier-config": "~4.0.0",
    "@types/jasmine": "~4.3.1",
    "@types/node": "^20.1.3",
    "@typescript-eslint/eslint-plugin": "~6.4.0",
    "@typescript-eslint/parser": "~6.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-jsdoc": "~46.5.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "husky": "~8.0.3",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "~14.0.1",
